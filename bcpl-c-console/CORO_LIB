GET "LIBHDR"

LET CORONEW(F, INIT) = VALOF
$( LET C = GETVEC(4)
   IF C=0 RESULTIS 0
   C!0 := INIT
   C!1 := 0
   C!2 := F
   C!3 := 0
   RESULTIS C
$)

AND CORONEXT(C) = VALOF
$( LET F = C!2
   IF F=0 RESULTIS 0
   RESULTIS F(C)
$)

AND COROSET(C,IDX,VAL) = VALOF
$( C!IDX := VAL
  RESULTIS VAL
$)

AND COROGET(C,IDX) = VALOF
$( RESULTIS C!IDX
$)
